/*
 Repeat Programming Exercise 3 by declaring num1, num2, and num3, and average of type double. Store 75.35 into num1, -35.56 into num2, and 15.76 into num3.
 */

 #include <iostream>

 using namespace std;

 int main ()
 {
     double num1 = 75.35;
     double num2 = -35.56;
     double num3 = 15.76;
     double average;
     
     average = (num1 + num2 + num3) / 3;
     
     cout << "num1 = " << num1 << endl;
     cout << "num2 = " << num2 << endl;
     cout << "num3 = " << num3 << endl;
     cout << "Average = " << average << endl;
     
     return 0;
 }
